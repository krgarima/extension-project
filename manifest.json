{
  "manifest_version": 3,
  "name": "Efficacious",
  "version": "1.0",
  "description": "A browser extension that will help you focus and increase your work productivity",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["geolocation", "storage"]
}
